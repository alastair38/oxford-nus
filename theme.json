{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "version": 2,
  "settings": {
    "color": {
      "defaultPalette": false,
      "palette": [
        {
          "slug": "base",
          "color": "hsl(0deg 0% 100%)",
          "name": "Base"
        },
        {
          "slug": "contrast",
          "color": "hsl(215deg 94% 26%) ",
          "name": "Contrast"
        },
        {
          "slug": "primary",
          "color": "hsl(31deg 100% 47%)",
          "name": "Primary"
        },
        {
          "slug": "secondary",
          "color": "hsl(188deg 76% 57.5%)",
          "name": "Secondary"
        },
        {
          "slug": "neutral-light-900",
          "color": "hsl(220deg 14.3% 86%)",
          "name": "Neutral Light (900)"
        },
        {
          "slug": "neutral-light-500",
          "color": "hsl(220deg 14.3% 91%)",
          "name": "Neutral Lighter (500)"
        },
        {
          "slug": "neutral-light-100",
          "color": "hsl(220deg 14.3% 95.9%)",
          "name": "Neutral Lightest (100)"
        },
        {
          "slug": "neutral-dark-100",
          "color": "hsl(0deg 0% 36%)",
          "name": "Neutral Dark (100)"
        },
        {
          "slug": "neutral-dark-500",
          "color": "hsl(0deg 0% 31%)",
          "name": "Neutral Darker (500)"
        },
        {
          "slug": "neutral-dark-900",
          "color": "hsl(0deg 0% 26.7%)",
          "name": "Neutral Darkest (900)"
        }
      ],
      "gradients": [
        {
          "slug": "gray-to-cyan",
          "gradient": "linear-gradient(90deg, var(--wp--preset--color--neutral-light-100), var(--wp--preset--color--secondary))",
          "name": "Gray to cyan"
        },
        {
          "slug": "white-to-sky",
          "gradient": "linear-gradient(90deg, hsla(0, 0%, 88%, 1) 0%, hsla(190, 100%, 90%, 1) 100%)",
          "name": "White to sky"
        },
        {
          "slug": "dark-to-pink",
          "gradient": "linear-gradient(to right, #C06C84, #6C5B7B, #355C7D)",
          "name": "Dark blue to pink"
        }
      ],
      "custom": false,
      "customGradient": false,
      "defaultGradients": false
    },
    "custom": {
      "spacing": {
        "baseline": "1.5rem"
      }
    },
    "spacing": {
      "blockGap": true,
      "margin": true,
      "padding": true
    },
    "typography": {
      "fluid": true,
      "customFontSize": false,
      "lineHeight": true,
      "dropCap": false,
      "fontFamilies": [
        {
          "fontFamily": "-apple-system,BlinkMacSystemFont,\"Segoe UI\",Roboto,Oxygen-Sans,Ubuntu,Cantarell,\"Helvetica Neue\",sans-serif",
          "slug": "system-fonts",
          "name": "System Fonts"
        },
        {
          "fontFamily": "Montserrat, sans-serif",
          "slug": "montserrat",
          "name": "Sans serif",
          "fontFace": [
            {
              "fontFamily": "Montserrat",
              "fontWeight": "400",
              "fontStyle": "normal",
              "src": ["file:./assets/fonts/Montserrat-Regular.woff"]
            },
            {
              "fontFamily": "Montserrat",
              "fontWeight": "800",
              "fontStyle": "normal",
              "src": ["file:./assets/fonts/Montserrat-Bold.woff"]
            },
            {
              "fontFamily": "Montserrat",
              "fontWeight": "400",
              "fontStyle": "italic",
              "src": ["file:./assets/fonts/Montserrat-Italic.woff"]
            },
            {
              "fontFamily": "Montserrat",
              "fontWeight": "800",
              "fontStyle": "italic",
              "src": ["file:./assets/fonts/Montserrat-BoldItalic.woff"]
            }
          ]
        },
        {
          "fontFamily": "Merriweather, Cambria, Georgia, serif",
          "slug": "serif",
          "name": "Serif",
          "fontFace": [
            {
              "fontFamily": "Merriweather",
              "fontWeight": "400",
              "fontStyle": "normal",
              "src": ["file:./assets/fonts/merriweather-v30-latin-regular.woff"]
            },
            {
              "fontFamily": "Merriweather",
              "fontWeight": "900",
              "fontStyle": "normal",
              "src": ["file:./assets/fonts/merriweather-v30-latin-900.woff"]
            }
          ]
        }
      ],
      "fontSizes": [
        {
          "size": "0.5rem",
          "fluid": {
            "min": "0.5rem",
            "max": "0.72rem"
          },
          "slug": "tiny",
          "name": "Tiny"
        },
        {
          "size": "0.7rem",
          "fluid": {
            "min": "0.7rem",
            "max": "0.9rem"
          },
          "slug": "small",
          "name": "Small"
        },
        {
          "size": "0.88rem",
          "fluid": {
            "min": "0.88rem",
            "max": "1.13rem"
          },
          "slug": "normal",
          "name": "Normal"
        },
        {
          "size": "1.09rem",
          "fluid": {
            "min": "1.09rem",
            "max": "1.41rem"
          },
          "slug": "large",
          "name": "Large"
        },
        {
          "size": "1.37rem",
          "fluid": {
            "min": "1.37rem",
            "max": "1.76rem"
          },
          "slug": "extra-large",
          "name": "Extra Large"
        },
        {
          "size": "1.71rem",
          "fluid": {
            "min": "1.71rem",
            "max": "2.20rem"
          },
          "slug": "huge",
          "name": "Huge"
        },
        {
          "size": "2.14rem",
          "fluid": {
            "min": "2.14rem",
            "max": "2.75rem"
          },
          "slug": "gigantic",
          "name": "Gigantic"
        },
        {
          "size": "3.14rem",
          "fluid": {
            "min": "3.14rem",
            "max": "4.75rem"
          },
          "slug": "display",
          "name": "Display"
        }
      ]
    },
    "layout": {
      "contentSize": "800px",
      "wideSize": "1100px"
    },
    "appearanceTools": true
  },
  "styles": {
    "color": {
      "background": "var(--wp--preset--color--base)",
      "text": "var(--wp--preset--color--contrast)"
    },
    "spacing": {
      "blockGap": "1.5rem"
    },
    "typography": {
      "fontSize": "var(--wp--preset--font-size--normal)",
      "fontFamily": "var(--wp--preset--font-family--montserrat)"
    },
    "elements": {
      "button": {
        ":focus": {
          "outline": {
            "offset": "2px",
            "width": "2px",
            "style": "solid",
            "color": "var(--wp--preset--color--contrast)"
          }
        }
      },
      "heading": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--montserrat)"
        }
      },
      "h1": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--huge)",
          "fontFamily": "var(--wp--preset--font-family--montserrat)"
        }
      }
    },
    "blocks": {
      "core/button": {
        "color": {
          "background": "transparent",
          "text": "currentColor"
        },
        "spacing": {
          "padding": {
            "top": "0.5rem",
            "right": "0.75rem",
            "bottom": "0.5rem",
            "left": "0.75rem"
          }
        },
        "border": {
          "radius": "inherit",
          "width": "0px",
          "color": "currentColor"
        },
        "typography": {
          "fontSize": "var(--wp--preset--font-size--normal)"
        }
      },
      "core/image": {
        "spacing": {
          "margin": {
            "top": "0px",
            "right": "0px",
            "bottom": "0px",
            "left": "0px"
          }
        }
      },
      "core/navigation-link": {
        "color": {
          "text": "var(--wp--preset--color--contrast)"
        },
        "border": {
          "radius": "30px",
          "color": "var(--wp--preset--color--contrast)",
          "width": "1px"
        }
      },
      "core/group": {
        "spacing": {
          "blockGap": "0rem"
        }
      },
      "core/query": {
        "spacing": {
          "blockGap": "0rem"
        }
      },
      "core/post-template": {
        "spacing": {
          "blockGap": "0rem"
        }
      },
      "core/separator": {
        "spacing": {
          "padding": {
            "top": "1rem",
            "right": "0px",
            "bottom": "0px",
            "left": "0px"
          }
        },
        "border": {
          "color": "var(--wp--preset--color--contrast)",
          "width": "1px"
        }
      },
      "core/post-terms": {
        "color": {
          "text": "var(--wp--preset--color--contrast)"
        }
      },
      "core/social-links": {
        "spacing": {
          "blockGap": "0rem",
          "margin": {
            "top": "0rem",
            "right": "0rem",
            "bottom": "0rem",
            "left": "0rem"
          }
        }
      },
      "core/quote": {
        "spacing": {
          "margin": {
            "left": "auto",
            "right": "auto"
          }
        }
      }
    }
  }
}
